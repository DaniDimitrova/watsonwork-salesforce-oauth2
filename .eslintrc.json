{
  "parser": "espree",
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "jasmine": true,
    "mocha": true
  },
  "globals": {
    "__DEV__": true,
    "sinon": true,
    "chai": true,
    "spy": true,
    "stub": true
  },
  "parserOptions": {
    "sourceType": "module"
  },
  "rules": {
    "space-before-blocks": 2,
    "semi": [2, "always"],
    "comma-dangle": [2, "never"],
    "no-unused-expressions": 2,
    "block-scoped-var": 2,
    "dot-notation": 2,
    "consistent-return": 2,
    "no-unused-vars": [2, {"args": "none"}],
    "quotes": [2, "single"],
    "space-infix-ops": 2,
    "no-else-return": 2,
    "no-extra-parens": 2,
    "no-eq-null": 2,
    "no-floating-decimal": 2,
    "no-param-reassign": 2,
    "no-self-compare": 2,
    "wrap-iife": [2, "inside"],
    "brace-style": 2,
    "object-curly-spacing": [1, "always"],
    "func-style": [2, "expression"],
    "no-lonely-if": 2,
    "space-in-parens": [2, "never"],
    "space-before-function-paren": [2, "never"],
    "generator-star-spacing": [2, "before"],
    "spaced-comment": [2, "always"],
    "eol-last": 2,
    "no-multi-spaces": 2,
    "curly": [2, "all"],
    "camelcase": [2, {"properties": "never"}],
    "no-eval": 2,
    "no-var": 2,
    "max-len": [2, 100],
    "complexity": [1, 12],
    "arrow-parens": [2, "always"],
    "no-use-before-define": 1,
    "no-underscore-dangle": 0,
    "new-cap": 0,
    "key-spacing": 0,
    "space-after-keywords": 0,
    "no-shadow": 0,
    "space-in-brackets": 0
  }
}
